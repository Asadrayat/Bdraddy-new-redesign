                            <div class="swiper-slide thumb-slide">
                              {% case media.media_type %}
                                {% when 'image' %}
                                  <div
                                    data-media="{{ media.id }}"
                                    class="pdp-media pdp-media-image thumb-media"
                                  >
                                    {{ media | image_url: width: 100 | image_tag: class: 'pdp__thumb-tag', loading: 'lazy' }}
                                  </div>
                                {% when 'video' %}
                                  <div
                                    data-media="{{ media.id }}"
                                    class="pdp-media pdp-media-video thumb-media"
                                  >
                                    {{ media.preview_image | image_url: width: 100 | image_tag: class: 'pdp__thumb-tag', loading: 'lazy' }}
                                  </div>
                              {% endcase %}
                            </div>
                        {% else %}
                          {% if forloop.index0 >= start_index and forloop.index0 <= end_index %}
                            <div class="swiper-slide thumb-slide">
                              {% case media.media_type %}
                                {% when 'image' %}
                                  <div
                                    data-media="{{ media.id }}"
                                    class="pdp-media pdp-media-image thumb-media"
                                  >
                                    {{ media | image_url: width: 100 | image_tag: class: 'pdp__thumb-tag', loading: 'lazy' }}
                                  </div>
                                {% when 'video' %}
