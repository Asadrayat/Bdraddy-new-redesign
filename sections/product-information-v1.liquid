<section class="--product-section" id="--custom-product-section">
{% # render 'pfm', section: section %}
{% comment %} <script src="{{ 'product-information-v1.js' | asset_url }}" defer="defer"></script>{% endcomment %}
{% comment %} {% liquid
  assign id = section.id

  for option in product.options_with_values
    if option.name == 'Color' or option.name == 'color' or option.name == 'COLOR'
      assign color_option = 'option' | append: option.position
    endif
  endfor
  assign selected_color = product.selected_or_first_available_variant[color_option] | downcase
  assign is_selected_variant = product.selected_or_first_available_variant.id

  assign is_alt_text = false
  for media in product.media
    assign alt_txt = media.alt | split: '#' | first | strip | downcase
    if alt_txt == selected_color
      assign is_alt_text = true
    endif
  endfor
%} {% endcomment %}
{% liquid
  assign id = section.id
  assign color_option = 'option1'
  
  assign selected_color = product.selected_or_first_available_variant[color_option] | downcase
  assign is_selected_variant = product.selected_or_first_available_variant.id

  assign is_alt_text = false
  for media in product.media
    assign alt_txt = media.alt | split: '#' | first | strip | downcase
    if alt_txt == selected_color
      assign is_alt_text = true
    endif
  endfor
%}


<style>
  .sec-{{ id }} {
    box-sizing: border-box;
    margin-top: {{section.settings.margin_top | append: 'px'}};
    margin-bottom: {{section.settings.margin_bottom | append: 'px'}};
    padding-top: {{section.settings.padding_top | append: 'px'}};
    padding-bottom: {{section.settings.padding_bottom | append: 'px'}};
  }
  {% if template.suffix == 'gift-card' %}
  .sec-{{ id }} .pr-faq-item {
    padding-bottom: 16px;
  }    
  {% endif %}
  @media(max-width: 600px) {
    .sec-{{ id }} {
      margin-top: {{section.settings.margin_top_m | append: 'px'}};
      margin-bottom: {{section.settings.margin_bottom_m | append: 'px'}};
      padding-top: {{section.settings.padding_top_m | append: 'px'}};
      padding-bottom: {{section.settings.padding_bottom_m | append: 'px'}};
    }
  }

  {% if product.handle == 'the-joey-2-pack' %}
    .sale__discount-variant{
      display: none;
    }
  {% endif %}
  .sec-{{ id }} .pinfo-wrapper.page-width {
    @media(min-width: 1600px){
      max-width: 1900px;
    }
  }
</style>
<div id="product-information-container" data-id="{{ section.id }}" class="sec-{{ id }}">
  {% comment %} {{ 'product-information-v1.css' | asset_url | stylesheet_tag }} {% endcomment %}
  <product-information data-update-url="true" data-section-id="{{ section.id }}">
     
    <div class="pinfo-wrapper page-width">
      {% if product.metafields.custom.action_video != blank %}
        {% render 'see-action', product: product, has_model_info: has_model_info %}
      {% endif %}
      <pinfo-content-sm class="sm-view">
        <div class="pinfo-top mb">
          {% for block in section.blocks %}
            {% case block.type %}
              {% when 'description' %}
                {% unless product.metafields.custom.description_romance_copy.value == blank %}
                  <div class="sm-view product__description {% if block.settings.enable_mb %}mb{% endif %}">{{ block.settings.desc }}</div>
                {% endunless %}                    
              {% comment %} {% when '@app' %}
                <div class="pinfo-app-block">{% render block %}</div> {% endcomment %}
              
              {% when 'title' %}
                <div>
                  {% for block in section.blocks %}
                    {% if block.type == 'breadcrumbs' %}
                      {% #render 'breadcrumb', product: product, block: block, class: 'sm-view' %}   
                    {% endif %}
                   {% endfor %} 
                  <div class="sm-view pd-title-wrapper {% if block.settings.enable_mb %}mb{% endif %}">
                    <h1 class="pinfo-title h2">{{ product.title | downcase }}</h1>
                  </div>
                  <div class="--product-attribute-wrapper sm-view">
                    {% if product.metafields.custom.pattern_type.value[0] != blank %}
                      <p class="--product-attribute">{{ product.metafields.custom.pattern_type.value | join : " " }}</p>
                    {% endif %}
                    {% if product.metafields.custom.unique_element.value[0] != blank %}
                      <p class="--product-attribute">{{ product.metafields.custom.unique_element.value | join : " " }}</p>
                    {% endif %}
                  </div>
                  {% render 'pinfo-price-v1', product: product, block: block, class: 'sm-view' %} 
                </div>
              
              {% when 'price' %}
                <div class="price__collection_wrap mb">
                  <div class="mobile__only app__block mobile__review_wrap">
                    {% for block in section.blocks %}
                      {% if block.type == '@app' %}
                        <div class="pinfo-app-block">{% render block %}</div>
                      {% endif %}
                    {% endfor %}
                  </div>  
                  {% comment %} {% if product.metafields.custom.outfit_products.value != blank %}
                    <a href="#shop_the_look" class=" --shop-the-look-anchor --section-navigator mobile__only">Style It With</a>
                  {% endif %} {% endcomment %}
                  {% comment %} {% unless block.settings.discounted_text == blank  %}<p class="highlighted_discounted_text sm-view">{{ block.settings.discounted_text }}</p>{% endunless %}   {% endcomment %}               
                </div>
              {% when 'custom_discount_price' %}
                {% render 'custom-discount-price', product: product, block: block, section: section, class: 'sm-view' %}                                    
              {% when 'attribute' %}
                {% render 'product-attribute', product: product, block: block, section: section %}                
              {% comment %} {% when '@app' %}              
                  <div class="pinfo-app-block mb sm-view">{% render block %}</div> {% endcomment %}
            {% endcase %}
          {% endfor %}
        </div>
      </pinfo-content-sm>
      <product-info-media>
        {% if product.metafields.custom.outfit_products.value != blank %}
        <a href="#shop_the_look" class="--shop-the-look-anchor --section-navigator desktop__only">Style It With</a>
        {% endif %}
        {% case section.settings.image_style %}
          {% when 'slider' %}
            <div class="pinfo-slider-wrapper">
              <div class="swiper pinfo-thumb-swiper">
                <div class="swiper-wrapper">
                  {% for media in product.media %}
                    <div data-media="{{ media.id }}" class="swiper-slide pdp-media">
                      {% unless media == blank %}
                        <picture class="pinfo-thumb-pic">
                          <source media="(max-width: 600px)" srcset="{{ media | image_url: width: 500 }}">
                          <source media="(max-width: 1024px)" srcset="{{ media | image_url: width: 1024 }}">
                          {% if forloop.first %}
                            {{ media | image_url: width: media.width | image_tag }}
                          {% else %}
                            {{ media | image_url: width: media.width | image_tag: loading: 'lazy' }}
                          {% endif %}
                        </picture>
                      {% endunless %}
                    </div>
                  {% endfor %}
                </div>
              </div>
              <div class="swiper pinfo-gallery-swiper">
                <div class="swiper-wrapper">
                  {% for media in product.media %}
                    <div class="swiper-slide">
                      {% unless media == blank %}
                        <picture class="pinfo-gallery-pic">
                          <source
                            media="(max-width: 600px)"
                            srcset="{{ media | image_url: width: 400, height: 200, crop: 'top'}}"
                          >
                          {% if forloop.index >= 2 %}
                            {{ media | image_url: width: 500, height: 200, crop: 'top' | image_tag: loading: 'lazy' }}
                          {% else %}
                            {{ media | image_url: width: 500, height: 200, crop: 'top' | image_tag }}
                          {% endif %}
                        </picture>
                      {% endunless %}
                    </div>
                  {% endfor %}
                </div>
              </div>
            </div>
          {% when 'stack' %}

            {% liquid
              assign media_size = product.media.size
              assign mod = media_size | modulo: 2
              if mod == 0
                assign is_even = true
              else
                assign is_even = false
              endif
              assign featured_media_id = product.selected_or_first_available_variant.featured_media.id
              assign start_index = 0
              for m in product.media
                if m.id == featured_media_id
                  assign start_index = forloop.index0
                  break
                endif
              endfor
              assign end_index = start_index | plus: 0
              assign index_difference = end_index | minus: start_index | plus: 1


              assign has_featured_image = false
              if product.selected_or_first_available_variant.featured_media != blank
                assign has_featured_image = true
              endif
              
              assign show_all_images = false
              if is_alt_text == false or has_featured_image == false
                assign show_all_images = true
              endif
            %}

            {% liquid
              assign count_condition_1 = 0
              assign count_condition_2 = 0

              for m in product.media
                assign alt_text = m.alt | split: '#' | first | strip | downcase
                if is_alt_text and alt_text == selected_color and is_selected_variant != blank
                  assign count_condition_1 = count_condition_1 | plus: 1
                endif
                if is_alt_text == false or is_selected_variant == blank
                  assign count_condition_2 = count_condition_2 | plus: 1
                endif
              endfor

              assign is_condition_1_odd = false
              assign is_condition_2_odd = false

              if count_condition_1 > 0
                assign remainder_1 = count_condition_1 | modulo: 2
                if remainder_1 == 1
                  assign is_condition_1_odd = true
                endif
              endif

              if count_condition_2 > 0
                assign remainder_2 = count_condition_2 | modulo: 2
                if remainder_2 == 1
                  assign is_condition_2_odd = true
                endif
              endif
            %}

            {% render 'gallery-grid-styles' count_condition_1: count_condition_1, count_condition_2: count_condition_2, index_difference: index_difference %}

            <div class="count--{{ count_condition_2 }} stack___pdp-container-wrap {{ count_condition_1 }}---{{ count_condition_2 }}--{{ index_difference }}">
              <div class="pdp__media-swiper vertical__swiper stack___pdp-container">
                  {% if product.metafields.custom.action_video != blank %}
                    {% render 'see-action-button', product: product, has_model_info: has_model_info %}
                  {% endif %}
                <div class="vertical__medias test">
                  <div class="vertical__medias-wrapper">
                    {% for m in product.media %}
                      {% liquid
                        assign alt_text = m.alt | split: '#' | first | strip | downcase
                        assign media = m
                        if forloop.first and product.selected_or_first_available_variant.featured_media != blank and m.media_type == 'image'
                          assign media = product.selected_or_first_available_variant.featured_media
                        endif
                      %}

                      {% if is_alt_text and alt_text == selected_color and is_selected_variant != blank %}
                        {% case media.media_type %}
                          {% when 'image' %}
                            <div
                              
                              data-media="{{ media.id }}"
                              class="pdp-media-v info-media-image-v {% if forloop.first %}first{% endif %} {% if forloop.last and is_even %}last{% endif %}"
                            >
                              <picture class="pinfo-stack-picture-v">
                                 <source
                                    media="(max-width: 600px)"
                                    srcset="{{ media | image_url: width: 50 }}"
                                  >
                                  <source
                                    media="(max-width: 1600px)"
                                    srcset="{{ media | image_url: width: 100 }}"
                                  >
                                  <source
                                    media="(max-width: 2000px)"
                                    srcset="{{ media | image_url: width: 150 }}"
                                  >
                                
                                {% if forloop.index >= 2 %}
                                  {{ media | image_url: width: 400 | image_tag: class: 'vertical__media', loading: 'lazy' }}
                                {% else %}
                                  {{ media | image_url: width: 400 | image_tag: class: 'vertical__media' }}
                                {% endif %}
                              </picture>
                            </div>
                          {% when 'video' %}
                            <div
                              data-media="{{ media.id }}"
                              class="pdp-media-v info-media-image-v info-media-video-v {% if forloop.first %}first{% endif %} {% if forloop.last and is_even %}last{% endif %}"
                            >
                              {{
                                media
                                | video_tag: class: 'vertical__media', image_size: '400x', autoplay: true, loop: true, muted: true, controls: false
                              }}
                            </div>
                        {% endcase %}
                      {% endif %}
                      {% if is_alt_text == false or is_selected_variant == blank %}
                        {% if show_all_images %}
                          {% case media.media_type %}
                            {% when 'image' %}
                              <div
                                
                                data-media="{{ media.id }}"
                                class="pdp-media-v info-media-image-v {% if forloop.first %}first{% endif %} {% if forloop.last and is_even %}last{% endif %}"
                              >
                                <picture class="pinfo-stack-picture-v">
                                  <source
                                    media="(max-width: 600px)"
                                    srcset="{{ media | image_url: width: 50 }}"
                                  >
                                  <source
                                    media="(max-width: 1600px)"
                                    srcset="{{ media | image_url: width: 100 }}"
                                  >
                                  <source
                                    media="(max-width: 2000px)"
                                    srcset="{{ media | image_url: width: 150 }}"
                                  >
                                  {% if forloop.index >= 2 %}
                                    {{ media | image_url: width: 400 | image_tag: class: 'vertical__media', loading: 'lazy' }}
                                  {% else %}
                                    {{ media | image_url: width: 400 | image_tag: class: 'vertical__media' }}
                                  {% endif %}
                                </picture>
                              </div>
                            {% when 'video' %}
                              <div
                                data-media="{{ media.id }}"
                                class="pdp-media-v info-media-image-v info-media-video-v {% if forloop.first %}first{% endif %} {% if forloop.last and is_even %}last{% endif %}"
                              >
                                {{
                                  media
                                  | video_tag: class: 'vertical__media', image_size: '400x', autoplay: true, loop: true, muted: true, controls: false
                                }}
                              </div>
                          {% endcase %}
                        {% else %}
                          {% if forloop.index0 >= start_index and forloop.index0 <= end_index %}
                            {% case media.media_type %}
                              {% when 'image' %}
                                <div
                                  
                                  data-media="{{ media.id }}"
                                  class="pdp-media-v info-media-image-v {% if forloop.first %}first{% endif %} {% if forloop.last and is_even %}last{% endif %}"
                                >
                                  <picture class="pinfo-stack-picture-v">
                                    <source
                                      media="(max-width: 600px)"
                                      srcset="{{ media | image_url: width: 50 }}"
                                    >
                                    <source
                                      media="(max-width: 1600px)"
                                      srcset="{{ media | image_url: width: 100 }}"
                                    >
                                    <source
                                      media="(max-width: 2000px)"
                                      srcset="{{ media | image_url: width: 150 }}"
                                    >
                                    {% if forloop.index >= 2 %}
                                      {{ media | image_url: width: 400 | image_tag: class: 'vertical__media', loading: 'lazy' }}
                                    {% else %}
                                      {{ media | image_url: width: 400 | image_tag: class: 'vertical__media' }}
                                    {% endif %}
                                  </picture>
                                </div>
                              {% when 'video' %}
                                <div
                                  data-media="{{ media.id }}"
                                  class="pdp-media-v info-media-image-v info-media-video-v {% if forloop.first %}first{% endif %} {% if forloop.last and is_even %}last{% endif %}"
                                >
                                  {{
                                    media
                                    | video_tag: class: 'vertical__media', image_size: '400x', autoplay: true, loop: true, muted: true, controls: false
                                  }}
                                </div>
                            {% endcase %}
                          {% endif %}
                        {% endif %}
                      {% endif %}
                    {% endfor %}
                  </div>
                </div>
                <div class="{% if count_condition_2 > 0 %}info-media-stack-wrapper-no-alt{% endif %} info-media-stack-wrapper pdp-media-lg swiper-wrapper- {% if is_condition_1_odd or is_condition_2_odd %}odd__images{% endif %}">
                  {% for m in product.media %}
                    {% liquid
                      assign alt_text = m.alt | split: '#' | first | strip | downcase
                      assign media = m
                      if forloop.first and product.selected_or_first_available_variant.featured_media != blank and m.media_type == 'image'
                        assign media = product.selected_or_first_available_variant.featured_media
                      endif
                    %}

                    {% if is_alt_text and alt_text == selected_color and is_selected_variant != blank %}
                      {% case media.media_type %}
                        {% when 'image' %}
                          <div
                            data-media="{{ media.id }}"
                            class="pdp-media info-media-image {% if forloop.first %}first{% endif %} {% if forloop.last and is_even %}last{% endif %} swiper-slide-"
                          >
                            <picture class="pinfo-stack-picture">
                              <source
                                media="(max-width: 600px)"
                                srcset="{{ media | image_url: width: 400 }}"
                              >
                              {% if forloop.index >= 2 %}
                                {{ media | image_url: width: 800 | image_tag: class: 'pdp__media-tag', loading: 'lazy' }}
                              {% else %}
                                {{ media | image_url: width: 800 | image_tag: class: 'pdp__media-tag' }}
                              {% endif %}
                            </picture>
                          </div>
                        {% when 'video' %}
                          <div
                            data-media="{{ media.id }}"
                            class="pdp-media info-media-image info-media-video {% if forloop.first %}first{% endif %} {% if forloop.last and is_even %}last{% endif %} swiper-slide-"
                          >
                            {{
                              media
                              | video_tag: class: 'pdp__media-tag', image_size: '400x', autoplay: true, loop: true, muted: true, controls: true
                            }}
                          </div>
                      {% endcase %}
                    {% endif %}
                    {% if is_alt_text == false or is_selected_variant == blank %}
                      {% if show_all_images %}
                          {% case media.media_type %}
                            {% when 'image' %}
                              <div
                                data-media="{{ media.id }}"
                                class="pdp-media info-media-image info-media-image-no-alt {% if forloop.first %}first{% endif %} {% if forloop.last and is_even %}last{% endif %}"
                              >
                                <picture class="pinfo-stack-picture">
                                  <source
                                    media="(max-width: 600px)"
                                    srcset="{{ media | image_url: width: 400 }}"
                                  >
                                  {% if forloop.index >= 2 %}
                                    {{ media | image_url: width: 800 | image_tag: class: 'pdp__media-tag', loading: 'lazy' }}
                                  {% else %}
                                    {{ media | image_url: width: 800 | image_tag: class: 'pdp__media-tag' }}
                                  {% endif %}
                                </picture>
                              </div>
                            {% when 'video' %}
                              <div
                                data-media="{{ media.id }}"
                                class="pdp-media info-media-image info-media-image-no-alt info-media-video {% if forloop.first %}first{% endif %} {% if forloop.last and is_even %}last{% endif %}"
                              >
                                {{
                                  media
                                  | video_tag: class: 'pdp__media-tag', image_size: '400x', autoplay: true, loop: true, muted: true, controls: true
                                }}
                              </div>
                          {% endcase %}
                      {% else %}
                        {% if forloop.index0 >= start_index and forloop.index0 <= end_index %}
                          {% case media.media_type %}
                            {% when 'image' %}
                              <div
                                data-media="{{ media.id }}"
                                class="pdp-media info-media-image info-media-image-no-alt {% if forloop.first %}first{% endif %} {% if forloop.last and is_even %}last{% endif %}"
                              >
                                <picture class="pinfo-stack-picture">
                                  <source
                                    media="(max-width: 600px)"
                                    srcset="{{ media | image_url: width: 400 }}"
                                  >
                                  {% if forloop.index >= 2 %}
                                    {{ media | image_url: width: 800 | image_tag: class: 'pdp__media-tag', loading: 'lazy' }}
                                  {% else %}
                                    {{ media | image_url: width: 800 | image_tag: class: 'pdp__media-tag' }}
                                  {% endif %}
                                </picture>
                              </div>
                            {% when 'video' %}
                              <div
                                data-media="{{ media.id }}"
                                class="pdp-media info-media-image info-media-image-no-alt info-media-video {% if forloop.first %}first{% endif %} {% if forloop.last and is_even %}last{% endif %}"
                              >
                                {{
                                  media
                                  | video_tag: class: 'pdp__media-tag', image_size: '400x', autoplay: true, loop: true, muted: true, controls: true
                                }}
                              </div>
                          {% endcase %}
                        {% endif %}
                      {% endif %}
                    {% endif %}
                  {% endfor %}
                </div>
                {% comment %} <div class="swiper-scrollbar pdp_info_scrollbar"></div> {% endcomment %}
              </div>
            </div>

            {% liquid
              assign media_size = product.media.size
              assign featured_media_id = product.selected_or_first_available_variant.featured_media.id
              assign start_index = 0
              for m in product.media
                if m.id == featured_media_id
                  assign start_index = forloop.index0
                  break
                endif
              endfor
              assign end_index = start_index | plus: 3
            %}
            <div class="pdp-media-sm">
              <div class="main-swiper-wrapper">
   
                <div class="swiper main-swiper">
           {% if product.metafields.custom.outfit_products.value != blank %}
                <a href="#shop_the_look" class=" --shop-the-look-anchor --section-navigator mobile__only">Style It With</a>
              {% endif %}
                  {% if product.metafields.custom.action_video != blank %}
                    {% render 'see-action-button', product: product, has_model_info: has_model_info %}
                  {% endif %}
                  <div class="swiper-wrapper">
                    {% assign media_count = 0 %}
                    {% for media in product.media %}
                      {% liquid
                        assign alt_text = media.alt | split: '#' | first | strip | downcase
                        if forloop.first and product.selected_or_first_available_variant.featured_media != blank and media.media_type == 'image'
                          assign media = product.selected_or_first_available_variant.featured_media
                        endif
                      %}
                      {% if is_alt_text and alt_text == selected_color and is_selected_variant != blank %}
                        {% assign media_count = media_count | plus: 1 %}
                        <div class="swiper-slide">
                          {% case media.media_type %}
                            {% when 'image' %}
                              <div
                                data-media="{{ media.id }}"
                                class="pdp-media pdp-media-image"
                              >
                                <picture>
                                  <source
                                    media="(max-width: 600px)"
                                    srcset="{{ media | image_url: width: 400 }}"
                                  >
                                  {% if forloop.index >= 2 %}
                                    {{ media | image_url: width: 800 | image_tag: class: 'pdp__media-tag', loading: 'lazy' }}
                                  {% else %}
                                    {{ media | image_url: width: 800 | image_tag: class: 'pdp__media-tag' }}
                                  {% endif %}
                                </picture>
                              </div>
                            {% when 'video' %}
                              <div
                                data-media="{{ media.id }}"
                                class="pdp-media pdp-media-video"
                              >
                                {{
                                  media
                                  | video_tag: class: 'pdp__media-tag', image_size: '400x', autoplay: true, loop: true, muted: true, controls: true
                                }}
                              </div>
                          {% endcase %}
                        </div>
                      {% endif %}
                      {% if is_alt_text == false or is_selected_variant == blank %}
                        {% if show_all_images %}
                            {% assign media_count = media_count | plus: 1 %}
                            <div class="swiper-slide">
                              {% case media.media_type %}
                                {% when 'image' %}
                                  <div
                                    data-media="{{ media.id }}"
                                    class="pdp-media pdp-media-image"
                                  >
                                    <picture>
                                      <source
                                        media="(max-width: 600px)"
                                        srcset="{{ media | image_url: width: 400 }}"
                                      >
                                      {% if forloop.index >= 2 %}
                                        {{ media | image_url: width: 800 | image_tag: class: 'pdp__media-tag', loading: 'lazy' }}
                                      {% else %}
                                        {{ media | image_url: width: 800 | image_tag: class: 'pdp__media-tag' }}
                                      {% endif %}
                                    </picture>
                                  </div>
                                {% when 'video' %}
                                  <div
                                    data-media="{{ media.id }}"
                                    class="pdp-media pdp-media-video"
                                  >
                                    {{
                                      media
                                      | video_tag: class: 'pdp__media-tag', image_size: '400x', autoplay: true, loop: true, muted: true, controls: true
                                    }}
                                  </div>
                              {% endcase %}
                            </div>
                        {% else %}
                          {% if forloop.index0 >= start_index and forloop.index0 <= end_index %}
                            {% assign media_count = media_count | plus: 1 %}
                            <div class="swiper-slide">
                              {% case media.media_type %}
                                {% when 'image' %}
                                  <div
                                    data-media="{{ media.id }}"
                                    class="pdp-media pdp-media-image"
                                  >
                                    <picture>
                                      <source
                                        media="(max-width: 600px)"
                                        srcset="{{ media | image_url: width: 400 }}"
                                      >
                                      {% if forloop.index >= 2 %}
                                        {{ media | image_url: width: 800 | image_tag: class: 'pdp__media-tag', loading: 'lazy' }}
                                      {% else %}
