      </product-info-media>
      <product-info-content>
        <div class="pinfo-top">
          {% for block in section.blocks %}
            {% case block.type %}
              {% when 'description' %}
                {% unless product.metafields.custom.description_romance_copy.value == blank %}
                  <div class="lg-view product__description {% if block.settings.enable_mb %}mb{% endif %}">{{ block.settings.desc }}</div>
                {% endunless %}
              {% when 'attribute' %}
                {% render 'product-attribute', product: product, block: block, section: section %}
              {% when 'breadcrumbs' %}
                {%# render 'breadcrumb', product: product, block: block, class: 'lg-view' %}
              {% when '@app' %}
                {% comment %} <div class="pinfo-app-block mb lg-view">{% render block %}</div> {% endcomment %}
              {% when 'title' %}
                <div class='{% if block.settings.enable_mb %}mb_{% endif %}'>
                  {% render 'breadcrumb', product: product, block: block, class: 'lg-view' %}
                  <div class="lg-view pd-title-wrapper">
                    <h1 class="pinfo-title h2">{{ product.title | downcase }}</h1>
                  </div>
                  <div class="--product-attribute-wrapper lg-view">
                    {% if product.metafields.custom.pattern_type.value[0] != blank %}
                      <p class="--product-attribute">{{ product.metafields.custom.pattern_type.value | join : " " }}</p>
                    {% endif %}
                    {% if product.metafields.custom.unique_element.value[0] != blank %}
                      <p class="--product-attribute">{{ product.metafields.custom.unique_element.value | join : " " }}</p>
                    {% endif %}
                  </div>
                </div>
              <div class="pinfo__price_app">
                {% for block in section.blocks %}
                  {% if block.type == '@app' %}
                    <div class="pinfo-app-block lg-view">{% render block %}</div>
                  {% endif %}
                  {% if block.type == 'price' %}
                     {% render 'pinfo-price-v1', product: product, block: block, class: 'lg-view' %}
                  {% endif %}
                  {% if block.type == 'custom_discount_price' %}
                    {% render 'custom-discount-price', product: product, block: block, section: section, class: 'lg-view' %}   
                  {% endif %}
                {% endfor %}
              </div>  
              {% when 'price' %}
                {% #render 'pinfo-price-v1', product: product, block: block, class: 'lg-view' %}
              {% when 'custom_discount_price' %}
                {%# render 'custom-discount-price', product: product, block: block, section: section, class: 'lg-view' %}   
                {% comment %} {% unless block.settings.discounted_text == blank  %}<p class="highlighted_discounted_text lg-view">{{ block.settings.discounted_text }}</p>{% endunless %} {% endcomment %}
            {% endcase %}
          {% endfor %}
        </div>
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'buy-button' %}
              {% render 'pinfo-buy-button', product: product, block: block, section: section %}
            {% when 'variant-picker' %}
              {% render 'pinfo-variant-picker-v1', product: product, block: block %}
              {% when 'discount-code' %}
              <div class="--discount-code-container" style="width: 100%;">
                  {% unless product.selected_or_first_available_variant.compare_at_price > product.selected_or_first_available_variant.price %}
                    {% render 'mclbx-copy-discount-code', product: product, block: block %}
                  {% endunless %}
                </div>
            {% when 'inventory' %}
              {% #render 'inventory-status', product: product, block: block, section: section %}
            {% when 'quantity' %}
              {% comment %} <div class="quantity-selector-wrapper {% if block.settings.enable_mb %}mb{% endif %}">
                {% comment %} <h4>QTY: </h4> {% endcomment %}
                <div class="quantity-selector" data-quantity-selector>
                  <button type="button" class="qty-btn decrease" aria-label="Decrease quantity">âˆ’</button>
                  <input type="number" class="qty-input" name="quantity" value="1" min="1" step="1">
                  <button type="button" class="qty-btn increase" aria-label="Increase quantity">+</button>
                </div>
              </div> {% endcomment %}
            {% when 'image' %}
              {% assign image = block.settings.img %}
              {% assign image_mobile = block.settings.img_m %}
            {% if image %}
              <div class="image-wrap pdp__img_custom__wrap {% if image_mobile %}small-hide{% endif %} mb">
                {{ image | image_url: width: image.width | image_tag }}
              </div>
            {% endif %}
            {% if image_mobile %}
              <div class="image-wrap pdp__img_custom__wrap large-up-hide medium-hide mb">
                {{ image_mobile | image_url: width: image_mobile.width | image_tag }}
              </div>
              {%endif %}    
                {% when 'recommendation_text' %}
                  <style>
                       
  {{  block.settings.custom_liquid }}

                  </style>
                  {% if block.settings.text != blank %}
                     <div class="--text-block--{{  block.id }} --recommendation-text" >{%render 'all-icons', name: 'dot' %}{{ block.settings.text }}</div>
                  {% endif %}
                  {% endcase %}
        {% endfor %}
        {% render 'pinfo-collapsible-v1', section: section %}
        {% for block in section.blocks %}
          {% case block.type %}
            {%- when 'bundle' -%}
              {%- render 'pinfo-bundle-products-v1', product: product, block: block -%}
            {%- when 'embroider_notice' -%}
              {% if product.tags contains 'EMBROIDER' or product.tags contains 'embroider' or product.tags contains 'Ryder Cup 2025' %}
                <div class="--embroidery-message-container">
                    {%- render 'custom-embroidery-message', product: product -%}
                </div>  
              {% endif %}
                                         
            {% when 'shipping_info' %}
              {%- unless product.tags contains 'EMBROIDER' or product.template_suffix == 'gift-card' -%}
                <div class="pd-shipping__info {% if block.settings.enable_mb %}mb{% endif %}">
                  {%- if product.selected_or_first_available_variant.available and product.selected_or_first_available_variant.price > 10000 -%}
                {%- render 'product-shipping', block: block, product: product -%}
                {%- endif -%}
              </div>
                 {%- endunless -%} 
              
            {% when 'gurantee_info' %}
              {%- unless product.tags contains 'EMBROIDER' -%}
                
                <div class="pd-gurantee__info {% if block.settings.enable_mb %}mb{% endif %}">
                  {% if product.selected_or_first_available_variant.available %}
                {% render 'product-gurantee', block: block, product: product %}
                {% endif %}
              </div>
              
                 {%- endunless -%} 
              
              
                {% when 'text' %}
                  <style>
                        .--text-block--{{  block.id }}{
                          {{  custom_liquid }}
                        }

                  </style>
                  {% if block.settings.text != blank %}
                     <div class="--text-block--{{  block.id }}" style="margin-block: 10px; font-size: 12px;">{{ block.settings.text }}</div>
                  {% endif %}
             
              {% comment %} {% unless   product.metafields.custom.style_no == blank%}
               <p class="--custom-style">STYLE # {{ product.metafields.custom.style_no.value }}</p>
              {% endunless %} {% endcomment %}
            {%when 'faqs' %}
              {% render 'faqs', block: block, product: product, id: id %}
            {% when 'icons' %}
              {% render 'custom-attribute-icons'%}
              
          {% endcase %}
        {% endfor %}

      
      </product-info-content>
      <div class="vartical__slider_pd"></div>
