                            {% when 'image' %}
                              <div
                                data-media="{{ media.id }}"
                                class="pdp-media pdp-media-image thumb-media"
                              >
                                {{ media | image_url: width: 100 | image_tag: class: 'pdp__thumb-tag', loading: 'lazy' }}
                              </div>
                            {% when 'video' %}
                              <div
                                data-media="{{ media.id }}"
                                class="pdp-media pdp-media-video thumb-media"
                              >
                                {{ media.preview_image | image_url: width: 100 | image_tag: class: 'pdp__thumb-tag', loading: 'lazy' }}
                              </div>
                          {% endcase %}
                        </div>
                      {% endif %}
                      {% if is_alt_text == false or is_selected_variant == blank %}
