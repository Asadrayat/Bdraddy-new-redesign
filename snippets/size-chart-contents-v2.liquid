<style>
    .size__popup{
      .--single-chart {
              padding-block: 0 var(--Spacing-Section-Section-Vertical-Padding, 48px);
          @media(max-width: 768px) {
            padding-block: 0 var(--Spacing-Section-Section-Vertical-Padding, 48px);
          }
      }
    p.--chart-content {
        color: #000;
        font-family: var(--font-family-Body, Gotham);
        font-size: var(--Font-Size-Paragraph-Medium, 14px);
        font-style: normal;
        font-weight: 400;
        line-height: var(--Line-Height-Paragraph-Medium, 22px);
        margin: 0;
    }      

      .--chart-measurements .--measurement-row:nth-child(odd){
          background: var(--Background-base-subtle, #F3F4F6);
      }
          .--measurement-row {
              display: grid;
              grid-template-columns: repeat(5,1fr);
              text-align: center;
              font-family: var(--font-family-Body, "Neue Plak");
              font-size: 16px;
              text-transform: uppercase;
              line-height: 20px;
              padding-block: var(--Spacing-Container-sm, 16px);
              @media(max-width:600px){
                /* font-size: var(--Font-Size-Paragraph-Regular, 16px);
                line-height: var(--Line-Height-Paragraph-Regular, 24px); */
              }
          }
         .--chart-measurements   .--measurement-heading.--measurement-row {
            background: #1F2937;
            color: var(--Content-Inverted-main, #F3F4F6);
        }


      /* Toggle Styling Start */

     .toggle-container {
                display: flex;
               justify-content: flex-end;
                align-items: center;
                font-family: Arial, sans-serif;
                font-size: 16px;
                @media(max-width: 600px){
                  align-items: flex-start;
                  grid-column: unset;
                  grid-row: unset;
                  justify-content: flex-start;
                }
            }

            .toggle-label {
                margin: 0 10px;
                color: #000;
                font-family: var(--font-family-Body, "Neue Plak");
                font-size: var(--Font-Size-Paragraph-Regular, 16px);
                line-height: var(--Line-Height-Paragraph-Regular, 20px);
                @media(max-width: 600px){
                  margin: 0 10px 0 0;
                }
            }

            .toggle-switch {
                position: relative;
                display: inline-block;
                width: 50px;
                height: 24px;
               @media(max-width: 600px){
                 width: 30px;
                 height: 15px;
                 margin-block: 2px;
               }
            }

            .toggle-switch input {
                opacity: 0;
                width: 0;
                height: 0;
            }

            .slider {
                position: absolute;
                cursor: pointer;
                top: 0;
                left: 0;
                right: 0;
                bottom: 0;
                background-color: #166534;
                transition: 0.4s;
                border-radius: 12px;
            }

            .slider:before {
                position: absolute;
                content: "";
                height: 18px;
                width: 18px;
                left: 3px;
                bottom: 3px;
                background-color: white;
                transition: 0.4s;
                border-radius: 50%;
               @media(max-width: 600px){
                 height: 10px;
                  width: 10px;
               }
            }

            input:checked + .slider {
                background-color: #166534;
            }

            input:checked + .slider:before {
                transform: translateX(26px);
               @media(max-width: 600px){
                 transform: translateX(14px);
               }
            }
    }
     /* Toggle Styling Start */

    .-chart-measurements table {
      border-collapse: collapse;
      width: 100%;
      font-family: Arial, sans-serif;
      text-align: center;
    }
    .-chart-measurements th, .-chart-measurements td {
      border: 1px solid #fff;
      padding: 12px;
    }
     .-chart-measurements th {
      background-color: #004225;
      color: #F3F4F6;
      font-weight: bold;
    }
    .-chart-measurements td {
      background-color: #e8eaed;
      color: #000;
    }
     .-chart-measurements .body-col {
      background-color: #004225;
      color: white;
      font-weight: bold;
    }

  .size-chart-wrapper {
    overflow-x: auto;
    -webkit-overflow-scrolling: touch;
    @media(max-width: 1050px)  {
    overflow-x: auto;
    -webkit-overflow-scrolling: touch;
    max-width: 90vw;
    box-sizing: border-box;      
    }
  }

  .size-chart {
    display: grid;
    grid-template-columns: repeat(6, minmax(120px, 1fr));
    font-family: Arial, sans-serif;
    text-align: center;
    border: 1px solid #fff;
  }
  .size-chart-wrapper.women .size-chart, .size-chart.men {
    grid-template-columns: repeat(5, minmax(120px, 1fr));
  }
  /* All cells */
  .size-chart > div {
      padding: 16px;
      border: 4px solid #fff;
      background-color: var(--Background-base-surface, #E5E7EB);
      box-sizing: border-box;
      color: var(--Content-Base-main, #1F2937);
      font-family: var(--font-family-Body, Satoshi);
      font-size: 16px;
      font-style: normal;
      font-weight: 700;
      line-height: var(--Line-Height-Paragraph-Large, 30px);
      @media(max-width: 768px) {
        padding: 8px;
        font-size: 13px;
        border: 2px solid #fff;
      }
  }

  /* Table__header row cells */
  .size-chart > .table__header {
    background: #1F2937;
    color: white;
    font-weight: bold;
    small {
      display: none;
    }
  }


  /* First column sticky */
  .size-chart > .body-col {
    background: #1F2937;
    color: white;
    font-weight: bold;
    position: sticky;
    left: 0;
    z-index: 2;
  }
  .body-col.second {
      left: 120px;
  }
  /* Top-left cell sticky in both directions */
  .size-chart > .body-col:first-child {
    top: 0;
    z-index: 3; /* Above the rest of the first column */
  }

  .size-chart-wrapper.women .size-chart > .body-col:first-child {    
    text-align: center;
    grid-column: 1/3;
  }
  .size-chart-wrapper.kids .size-chart {
      display: grid;
      grid-template-columns: repeat(4, minmax(120px, 1fr));
      font-family: Arial, sans-serif;
      text-align: center;
      border: 1px solid #fff;
      .body-col:first-child {
        {% comment %} grid-column: 1/3; {% endcomment %}
        text-align: center;
      }
  }
  .__size_img img{
      width: 100%;
      height: auto;
      object-fit: cover;
  }
      /* Toggle Styling ends */
</style>

