    </div>
    {% comment %}
      <div class="drawer__search hidden">
        {% render 'drawer-search' %}
      </div>
    {% endcomment %}
    <div class="drawer-menu">
      {% render 'drawer-menu' %}
    </div>
    <div class="drawer__utensils">
      <div class="drawer__account">
        <a account-drawer-- href="/account">{% if customer %}My Account{% else %}Sign In{% endif %}</a>
